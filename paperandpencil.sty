\newcommand{\answercontainervmargin}{0.25cm}

% Answers with the style A.
\newenvironment{answersA}
{
	\vspace{\answercontainervmargin}
	\begin{addmargin}[2cm]{6cm}
	\begin{compactitem}[]}{\end{compactitem}\end{addmargin}}

\newcommand{\pbox}[1]{#1\dotfill$\Box$}
\newcommand{\pbigbox}[1]{#1\dotfill$\square$}
\newcommand{\ebox}[1]{#1\hfill$\Box$}
\newcommand{\ebigbox}[1]{#1\hfill$\square$}

% Long answers with the style A.
\newenvironment{longanswersA}
{
	\vspace{\answercontainervmargin}
	\begin{addmargin}[0.5cm]{0cm}
	\begin{compactitem}[]}{\end{compactitem}\end{addmargin}}

% Answers with the style B.
\newenvironment{answersB}{
	\vspace{\answercontainervmargin}
	\begin{addmargin}[2cm]{6cm}
		\begin{compactitem}[$\Box$\quad]
		}{\end{compactitem}\end{addmargin}}

% Long answers with the style B.		
\newenvironment{longanswersB}{
	\vspace{\answercontainervmargin}
	\begin{addmargin}[0.5cm]{0cm}\begin{compactitem}[$\Box$\quad]}{\end{compactitem}\end{addmargin}}

% Answers with the style C.
\newenvironment{answersC}
{
	\vspace{\answercontainervmargin}
	\begin{addmargin}[2cm]{6cm}
	\begin{compactitem}[$\square$\quad]}{\end{compactitem}\end{addmargin}}

% Long answers with the style C.
\newenvironment{longanswersC}{
	\vspace{\answercontainervmargin}
	\begin{addmargin}[0.5cm]{0cm}
	\begin{compactitem}[$\square$\quad]}{\end{compactitem}\end{addmargin}}
 
 
